#include <stdio.h>
#define N 4 

int L[N]={2, 5, 6, 4};
int soma, somapares;

int main(){
    int i;
    soma = 0;
    somapares=0;
    for(i=0;i<N;i++){
        soma=soma+L[i];
        if(L[i]%2==0){
            somapares=somapares+L[i];
        }
    }
    
    printf("%d %d", soma, somapares);
    
    return 0;
}
